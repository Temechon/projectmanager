{
  "applicationId": "com.pixelcodr.projectmanager",
  "defaultMode": "browser",
  "port": 0,
  "url": "/resources/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeBlockList": [],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "projectmanager",
      "width": 800,
      "height": 500,
      "minWidth": 400,
